{
  "name": "svelte-experiment",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "experiment": "./config/createExperiment.sh",
    "prettier": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --write --plugin-search-dir=. ./**/*.html",
    "lint": "eslint --config ./.eslintrc.js --ext .js,.svelte --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn prettier",
      "yarn lint"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-yaml": "^3.0.0",
    "autoprefixer": "^9.7.6",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.0",
    "prettier-plugin-svelte": "^2.1.1",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^5.1.2",
    "sass": "^1.29.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.7.4"
  },
  "dependencies": {
    "amplitude-js": "^7.1.1",
    "gsap": "^3.5.1",
    "sirv-cli": "^0.4.4",
    "svelte-css-vars": "0.0.1"
  }
}